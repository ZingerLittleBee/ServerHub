{
  "name": "server-octopus",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "author": "",
  "license": "AGPLv3",
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "preinstall": "npx only-allow pnpm",
    "install:infra": "pnpm install --ignore-scripts --frozen-lockfile --filter shared --filter types",
    "install:server": "pnpm install:infra && pnpm install --ignore-scripts --frozen-lockfile --filter server",
    "build:infra": "pnpm --filter shared --filter types build",
    "build:view": "nx build @server-octopus/view --prod",
    "build:server": "pnpm build:infra && nx build @server-octopus/server --prod && nx run @server-octopus/server:build:pkg",
    "build:client": "pnpm build:infra && nx build @server-octopus/client --prod && nx run @server-octopus/client:build:pkg"
  },
  "devDependencies": {
    "nx": "16.6.0",
    "nx-cloud": "latest"
  }
}
